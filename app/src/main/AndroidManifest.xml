<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.ac.konkuk.timetable">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ThemeSettingActivity"
            android:label="@string/title_activity_theme_setting" />
        <activity
            android:name=".AppInfoActivity"
            android:label="@string/title_activity_app_info" />
        <activity
            android:name=".LectureInfoActivity"
            android:label="@string/title_activity_lecture_info" />
        <activity
            android:name=".LecturePlanActivity"
            android:label="@string/title_activity_lecture_plan" />
        <activity
            android:name=".LectureActivity"
            android:label="@string/title_activity_lecture" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>